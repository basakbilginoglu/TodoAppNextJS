{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "DATABASE_URL": "@your-database-url",
    "PRISMA_GENERATE": "true"
  },
  "buildCommand": "npm run build && npx prisma generate"
}
